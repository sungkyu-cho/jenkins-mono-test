{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": [
    "plugin:vue/vue3-essential"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "vue/multi-word-component-names": "off"
  }
} 